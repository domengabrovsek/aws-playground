{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es2022",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "rootDir": ".",
    "baseUrl": ".",
    "outDir": "./dist",
  }
}